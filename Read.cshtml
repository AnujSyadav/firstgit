@model List<TeamProjectPro.Models.EmpModel>
@{
    ViewBag.Title = "Read";
}

<h1>Employee Info</h1>

<table class="table table-bordered table-condensed table-hover table-responsive table-striped">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Designation</th>
        <th>DOB</th>
        <th>Hno</th>
        <th>Building</th>
        <th>Zipcode</th>
        <th>Technology</th>
        <th>Email</th>
        @*<th>Pass</th>*@
        <th>Language</th>
        <th>Edit</th>
        <th>Delete</th>
    </tr>

    @for (int i = 0; i < Model.Count; i++)
    {
<tr>
    <td>@Model[i].eid</td>
    <td>@Model[i].ename</td>
    <td>@Model[i].edesig</td>
    <td>@Model[i].Dob</td>
    <td>@Model[i].Hno</td>
    <td>@Model[i].Build</td>
    <td>@Model[i].Zipcode</td>
    <td>@Model[i].tname</td>
    <td>@Model[i].email</td>
    @*<td>@Model[i].pass</td>*@
    <td>@Model[i].langname</td>
    <td><a class="btn btn-warning" href="@Url.Action("Edit","Emp",new { @Model[i].eid })">Edit</a></td>
    <td><a class="btn btn-danger" href="@Url.Action("Delete","Emp",new { @Model[i].eid })">Delete</a></td>
</tr>

    }
</table>

